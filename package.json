{
  "name": "node-backend",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:Shtokarev/NodeBackend.git",
  "author": "Shtokarev Yuriy <shtokarev@gmail.com>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint  --ext .ts ./src",
    "fix": "eslint --fix  --ext .ts ./src",
    "build": "tsc",
    "prod": "tsc --sourceMap false",
    "prestart": "npm run build",
    "start": "yarn build && env-cmd -f ./dev/.env node dist/index.js",
    "test": "yarn build && env-cmd -f ./dev/.env jest src/tests/**",
    "ci_test": "env-cmd -f ./dev/.env jest src/tests/**",
    "test:tap": "env-cmd -f ./dev/.env yarn test --coverage --reporters=jest-tap-reporter"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,ts}"
    ],
    "verbose": true
  },
  "dependencies": {
    "@sentry/node": "5.24.1",
    "aws-sdk": "^2.755.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "env-cmd": "^10.1.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.6.2",
    "redis": "^3.0.2",
    "request": "^2.88.2",
    "request-promise": "^4.2.6",
    "stripe": "^8.96.0",
    "uuid": "^8.3.0",
    "uuidv4": "^6.2.3"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@sentry/types": "^5.24.1",
    "@types/bcrypt": "^3.0.0",
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.8",
    "@types/jasmine": "^3.5.14",
    "@types/jest": "^26.0.14",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongodb": "^3.5.27",
    "@types/node": "^14.11.1",
    "@types/redis": "^2.8.27",
    "@types/request-promise": "^4.1.46",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "babel-jest": "^26.3.0",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^24.0.1",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.4.2",
    "jest-tap-reporter": "^1.9.0",
    "nock": "^13.0.4",
    "prettier": "^2.1.2",
    "supertest": "^4.0.2",
    "typescript": "^4.0.2"
  }
}
